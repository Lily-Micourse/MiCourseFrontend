{
  "presets": [
    "next/babel",
    "@zeit/next-typescript/babel"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    [
      "babel-plugin-root-import",
      {
        "paths": [{
          "rootPathPrefix": "~",
          "rootPathSuffix": "."
        }, {
          "rootPathPrefix": "@",
          "rootPathSuffix": "src"
        }]
      }
    ]
  ]
}
